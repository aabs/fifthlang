    <PackageReference Include="coverlet.collector" Version="6.0.0" />
    <PackageReference Include="FluentAssertions" Version="7.0.0-alpha.4" />
    <PackageReference Include="FsCheck.Xunit" Version="3.0.0-rc3" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.9.0" />
    <PackageReference Include="xunit" Version="2.6.6" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.5.6" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\ast-generated\ast_generated.csproj" />
    <ProjectReference Include="..\..\src\ast-model\ast_model.csproj" />
    <ProjectReference Include="..\..\src\parser\parser.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Using Include="Xunit" />
