<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>ast_tests</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CodeSamples\class-definition.5th" />
    <None Remove="CodeSamples\destructuring.5th" />
    <None Remove="CodeSamples\overloading.5th" />
    <None Remove="CodeSamples\property-access.5th" />
    <None Remove="CodeSamples\recursive-destructuring.5th" />
