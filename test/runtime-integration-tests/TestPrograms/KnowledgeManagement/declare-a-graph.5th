alias x as <http://example.com/blah#>;

store home = sparql_store(<http://localhost:8080/graphdb>);

class Person in <x:>
{
    dob : datetime ;
    age : int ;
}

calculate_age(val : datetime): TimeSpan
{
    return datetime.Now() - val;
}

foo(a : int, n : string): void
{
    Person eric = new Person();
    graph ericKnowledge in <x:people> = <{
        d: datetime = new datetime(1926, 5, 14);
        eric.dob = d;
        eric.age = calculate_age(d);
    }>;
    home += ericKnowledge;
}
