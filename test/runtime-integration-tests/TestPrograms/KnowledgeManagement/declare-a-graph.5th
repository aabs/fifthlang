alias x as <http://example.com/blah#>;

class Person in <x:>
{
    dob : DateTimeOffset ;
    age : int ;
}

calculate_age(val : DateTimeOffset): TimeSpan
{
    return DateTimeOffset.Now - val;
}

foo(a : int, n : string): void
{
    eric : Person = new Person();
    d: DateTimeOffset = DateTimeOffset(1926, 5, 14, 0, 0, 0, TimeSpan.Zero);
    eric.dob = d;
    eric.age = calculate_age(d);
    ericKnowledge : graph in <x:people> = KG.CreateGraph();
}
