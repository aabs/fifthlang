<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<RootNamespace>ast_generated</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		<CompilerGeneratedFilesOutputPath>Generated</CompilerGeneratedFilesOutputPath>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\ast-model\ast_model.csproj" />
	  <ProjectReference Include="..\AstSourceGenerators\AstSourceGenerators.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
	</ItemGroup>

	<ItemGroup>
	  <AdditionalFiles Include="..\ast-model\AstMetamodel.cs" />
	  <AdditionalFiles Include="..\ast-model\ILMetamodel.cs" />
	</ItemGroup>

</Project>
