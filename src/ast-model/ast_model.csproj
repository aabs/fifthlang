<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks Condition="'$(EnableNet10)' == 'true'">net8.0;net10.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(EnableNet10)' != 'true'">net8.0</TargetFrameworks>
		<RootNamespace>ast_model</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <DefineConstants>$(DefineConstants)</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="dunet" Version="1.11.2" />
	  <PackageReference Include="Vogen" Version="6.0.0" />
	</ItemGroup>

</Project>
