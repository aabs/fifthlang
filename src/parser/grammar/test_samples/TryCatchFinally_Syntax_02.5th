// Multiple catch clauses with different forms (using simple type names)
main(): int {
    try {
        riskyOperation();
    } catch (ex: Exception) {
        std.print("Caught with id and type");
    } catch (IOException) {
        std.print("Caught with type only");
    } catch {
        std.print("Catch-all");
    }
    return 0;
}

riskyOperation(): void {
}
